#!/usr/bin/env bash

echo 'Start gRPC initialization...'
bin/magento storefront:grpc:init \\Magento\\CatalogStorefrontApi\\Api\\CatalogProxyServer \\Magento\\CatalogStorefrontApi\\Api\\VariantServiceProxyServerecho 'End of gRPC initialization'
